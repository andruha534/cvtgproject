n    font-size: 6vw;\n  }\n  \n  .auto-type {\n    color: #ff7f50;\n  }\n  \n  /* Заголовки, описания */\n  .title {\n    font-size: 5vw;\n    margin-bottom: 20px;\n    text-align: center;\n  }\n  \n  .description {\n    font-size: 1vw;\n    text-align: center;\n    margin-bottom: 2vw;\n  }\n  \n  /* Картинка (GIF) */\n  .cvgif {\n    border-radius: 15px;\n    box-shadow: 0 0 20px rgba(0, 0, 0, 0.5);\n    transition: transform 0.5s, box-shadow 0.5s;\n    margin-top: 6vw;\n    width: 100%;\n    max-width: 600px;\n    height: auto;\n  }\n  \n  .cvgif:hover {\n    transform: scale(1.05);\n    box-shadow: 0 0 30px rgba(0, 0, 0, 0.7);\n  }\n  \n  /* try now */\n  button {\n    position: relative;\n    padding: 12px 35px;\n    background: #ffffff;\n    font-size: 17px;\n    font-weight: 700;\n    color: #181818;\n    border: 3px solid #ffffff;\n    border-radius: 8px;\n    transition: all 0.3s ease-in-out;\n    cursor: pointer;\n  }\n  \n  /* STARS */\n  .star-1, .star-2, .star-3,\n  .star-4, .star-5, .star-6 {\n    position: absolute;\n    filter: drop-shadow(0 0 0 #fffdef);\n    z-index: -5;\n    width: 15px;\n    transition: all 1s cubic-bezier(0, 0.4, 0, 1.01);\n  }\n  \n  /* Индивидуальные размеры / позиционирование */\n  .star-1 { top: 20%; left: 20%; width: 25px; }\n  .star-2 { top: 45%; left: 45%; }\n  .star-3 { top: 40%; left: 40%; width: 5px; }\n  .star-4 { top: 20%; left: 40%; width: 8px; transition: all 0.8s; }\n  .star-5 { top: 25%; left: 45%; }\n  .star-6 { top: 5%;  left: 50%; width: 5px; transition: all 0.8s ease; }\n  \n  /* Hover-анимация */\n  button:hover {\n    background: transparent;\n    color: #ec9d64;\n    box-shadow: 0 0 25px #fec1958c;\n  }\n  \n  button:hover .star-1 {\n    top: -80%; left: -30%;\n    filter: drop-shadow(0 0 10px #fffdef);\n    z-index: 2;\n  }\n  button:hover .star-2 {\n    top: -25%; left: 10%;\n    filter: drop-shadow(0 0 10px #fffdef);\n    z-index: 2;\n  }\n  button:hover .star-3 {\n    top: 55%; left: 25%;\n    filter: d